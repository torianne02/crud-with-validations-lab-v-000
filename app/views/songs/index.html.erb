<% @songs.each do |song| %>
  <div class="song">
    <ul>
      <li><%= song.title %></li>
      <li><%= song.artist_name %></li>
      <li><%= song.genre %></li>
      <li><%= song.release_year %></li>
      <li><%= link_to "Delete", song, method: :delete, data: { confirm: "Really?" } %></li>
    </ul>
  </div>
<% end %>
